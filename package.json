{
  "name": "lubank",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/lvsouza/fbank-backend.git",
  "author": "lvsouza",
  "license": "MIT",
  "scripts": {
    "jest:test": "jest",
    "jest:watch": "jest --watchAll",
    "knex:seed": "knex --knexfile knexfile.ts seed:run",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:rollback-last": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile knexfile.ts migrate:rollback --all",
    "app:dev": "ts-node-dev --respawn --ignore-watch node_modules --no-notify src/server.ts"
  },
  "private": true,
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.5",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.5"
  }
}